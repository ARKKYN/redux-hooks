{
  "name": "redux-hook",
  "version": "1.0.0",
  "description": "React hook for using redux in your application",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:types && parcel build ./src/index.ts --detailed-report",
    "build:types": "tsc --emitDeclarationOnly",
    "analyse": "source-map-explorer dist/index.js dist/index.js.map"
  },
  "keywords": [
    "react",
    "redux",
    "hooks",
    "useRedux",
    "javascript",
    "typescript",
    "api"
  ],
  "alias": {
    "react": false,
    "redux": false
  },
  "author": "Param <paramsinghvc@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "react": "^16.8.6",
    "redux": "^4.0.1",
    "source-map-explorer": "^1.8.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "redux": "^4.0.1"
  }
}